{
  "common": {
    "name": "DeepDetect"
  },
  "homeComponent": {
    "contentType": "json",
    "title": "DeepDetect Platform - dev",
    "description": [
      "This is a dev platform",
      "You can modify its title and this description in public/config.json"
    ],
    "headerLinks": {
      "linkJupyter": "http://10.10.77.61:18104/code/lab",
      "linkMenus": [
        {
          "name": "Annotation Tool",
          "icon": "fas fa-external-link-alt",
          "url": "/annotations/"
        }
      ]
    }
  },
  "deepdetect": {
    "refreshRate": {
      "info": 10000,
      "training": 10000
    },
    "servers": [
      {
        "name": "private",
        "settings": {
          "path": "/api/private",
          "isWritable": true,
          "isHostable": true,
          "sameOrigin": true
        }
      },
      {
        "name": "public",
        "settings": {
          "path": "/api/public",
          "isWritable": false,
          "sameOrigin": true
        }
      },
      {
        "name": "eris",
        "settings": {
          "path": "/api/eris",
          "isWritable": true,
          "sameOrigin": true
        }
      },
      {
        "name": "neptune02",
        "settings": {
          "path": "/api/neptune02",
          "isWritable": true,
          "sameOrigin": true
        }
      },
      {
        "name": "training_test",
        "settings": {
          "path": "/api/training_test",
          "isWritable": true,
          "sameOrigin": true
        }
      },
      {
        "name": "training_test_2",
        "settings": {
          "path": "/api/training_test_2",
          "isWritable": true,
          "sameOrigin": true
        }
      },
      {
        "name": "ceres",
        "settings": {
          "path": "/api/ceres",
          "isWritable": true,
          "sameOrigin": true
        }
      }
    ],
    "services": {
      "defaultConfig": [
        {
          "modelName": "default",
          "modelConfig": {
            "description": "",
            "model": {
              "repository": ""
            },
            "mllib": "caffe",
            "type": "supervised",
            "parameters": {
              "input": {
                "connector": "image",
                "height": 300,
                "width": 300
              },
              "mllib": {
                "nclasses": 2,
                "gpu": true,
                "gpuid": 0
              }
            }
          }
        }
      ]
    },
    "chains": {
      "path": "/chains/"
    }
  },
  "videoExplorer": {
    "icon": "fa-photo-video",
    "title": "Video Explorer",
    "rootPath": "/data/alx/code/video_detect",
    "folders": {
      "output": "output/",
      "processed": "processed/",
      "thumbs": "thumbs/"
    },
    "chronoItemSelectors": {
      "showLabel": true,
      "cardTitle": "PK",
      "cardSubtitle": [
        {
          "label": "vitesse: ",
          "statsKey": "vitesse"
        },
        {
          "label": "cables: ",
          "statsKey": "cables",
          "format": "percentArray",
          "separator": " | "
        }
      ]
    }
  },
  "imaginate": {
    "default": {
      "display": {
        "initImages": {},
        "mode": "description",
        "okClass": "",
        "boundingBox": true,
        "segmentation": false,
        "separateSegmentation": false,
        "segmentationColors": ["#1b9e77", "#d95f02"],
        "chain": {
          "multicrop": false,
          "search_nn": 10,
          "random_crops": 10,
          "min_size_ratio": 0.5
        }
      },
      "threshold": {
        "confidence": 0.5,
        "controls": true,
        "controlSteps": [0.8, 0.41, 0.1]
      },
      "request": {
        "objSearch": false,
        "imgSearch": false,
        "best": false,
        "ctc": false,
        "blank_label": false
      },
      "code": {
        "display": {
          "importLib": true,
          "ddConfig": true,
          "postPredict": true,
          "hostname": null,
          "port": null
        }
      }
    },
    "services": [
      {
        "name": "testService",
        "settings": {
          "display": {
            "initImages": {},
            "mode": "description",
            "okClass": "",
            "boundingBox": true,
            "segmentation": false,
            "separateSegmentation": false,
            "segmentationColors": ["#1b9e77", "#d95f02"]
          },
          "threshold": {
            "confidence": 0.5,
            "controls": true,
            "controlSteps": [0.8, 0.5, 0.3]
          },
          "request": {
            "objSearch": false,
            "imgSearch": false,
            "best": false,
            "ctc": false,
            "blank_label": false
          }
        }
      }
    ]
  },
  "gpuInfo": {
    "servers": [
      {
        "name": "eris",
        "url": "/gpu/eris",
        "aliases": ["private", "public", "training"]
      },
      {
        "name": "neptune02",
        "url": "/gpu/neptune02"
      },
      {
        "name": "ceres",
        "url": "/gpu/ceres"
      },
      {
        "name": "jetson dev alx",
        "url": "/gpu/jetson",
        "type": "jetson",
        "externalLink": "http://192.168.0.46:8888"
      }
    ],
    "refreshRate": 5000
  },
  "modelRepositories": [
    {
      "name": "public",
      "nginxPath": "/models/public/",
      "jsonPath": "/json/models/public/",
      "systemPath": "/opt/platform"
    },
    {
      "name": "private",
      "nginxPath": "/models/private/",
      "jsonPath": "/json/models/private/",
      "systemPath": "/opt/platform",
      "hasFiles": true
    },
    {
      "name": "training",
      "nginxPath": "/models/training/",
      "jsonPath": "/json/models/training/",
      "systemPath": "/opt/platform",
      "isTraining": true
    }
  ],
  "dataRepositories": {
    "nginxPath": "/data/",
    "jsonPath": "/json/data/",
    "systemPath": "/opt/platform/",
    "maxDepth": 2
  },
  "componentBlacklist": ["LinkChat"],
  "placeholders": {}
}
