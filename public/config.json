{
  "homeComponent": {
    "contentType": "json",
    "title": "DeepDetect Platform",
    "description": "Welcome to deepdetect"
  },
  "deepdetect": {
    "infoRefreshRate": 10000,
    "servers": [
      {
        "name": "public",
        "settings": {
          "path": "/api/public",
          "isWritable": false
        }
      },
      {
        "name": "private",
        "settings": {
          "path": "/api/private",
          "isWritable": true
        }
      },
      {
        "name": "training",
        "settings": {
          "path": "/api/training",
          "isWritable": false
        }
      }
    ],
    "services": {
      "defaultConfig": [
        {
          "modelName": "detection_600",
          "modelConfig": {
            "description": "object detection service",
            "model": {
              "repository": "/opt/platform/models/detection_600/"
            },
            "mllib": "caffe",
            "type": "supervised",
            "parameters": {
              "input": {
                "connector": "image",
                "height": 300,
                "width": 300
              },
              "mllib": {
                "nclasses": 601,
                "gpu": true,
                "gpuid": 0
              }
            }
          }
        },
        {
          "modelName": "segmentation_150",
          "modelConfig": {
            "description": "object detection service",
            "model": {
              "repository": "/opt/platform/models/segmentation_150/"
            },
            "mllib": "caffe",
            "type": "supervised",
            "parameters": {
              "input": {
                "connector": "image",
                "height": 480,
                "width": 480
              },
              "mllib": {
                "nclasses": 150,
                "gpu": true,
                "gpuid": 0
              }
            }
          }
        },
        {
          "modelName": "ggnet",
          "modelConfig": {
            "description": "object detection service",
            "model": {
              "repository": "/opt/platform/models/ggnet/"
            },
            "mllib": "caffe",
            "type": "supervised",
            "parameters": {
              "input": {
                "connector": "image",
                "height": 224,
                "width": 224
              },
              "mllib": {
                "nclasses": 21,
                "gpu": true,
                "gpuid": 0
              }
            }
          }
        },
        {
          "modelName": "resnet_50",
          "modelConfig": {
            "description": "object detection service",
            "model": {
              "repository": "/opt/platform/models/resnet_50/"
            },
            "mllib": "caffe",
            "type": "supervised",
            "parameters": {
              "input": {
                "connector": "image",
                "height": 224,
                "width": 224
              },
              "mllib": {
                "nclasses": 21,
                "gpu": true,
                "gpuid": 0
              }
            }
          }
        }
      ]
    }
  },
  "imaginate": {
    "display": {
      "initImages": {},
      "mode": "description",
      "okClass": "",
      "boundingBox": true,
      "segmentation": false,
      "separateSegmentation": false,
      "segmentationColors": ["#1b9e77", "#d95f02"]
    },
    "threshold": {
      "confidence": 0.5,
      "controls": true,
      "controlSteps": [0.8, 0.5, 0.3]
    },
    "request": {
      "objSearch": false,
      "imgSearch": false,
      "best": false,
      "ctc": false,
      "blank_label": false
    }
  },
  "gpuInfo": {
    "gpuStatServer": "http://10.10.77.61:12345",
    "refreshRate": 5000
  },
  "modelRepositories": {
    "nginxPath": {
      "public": "/models/public/",
      "private": "/models/private/"
    },
    "systemPath": {
      "public": "/opt/platform/models/public/",
      "private": "/opt/platform/models/private/"
    }
  },
  "dataRepositories": {
    "nginxPath": "/data/",
    "systemPath": "/opt/platform/data/"
  }
}
