{
  "name": "core-ui",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.0",
    "pretty-quick": "^1.6.0",
    "react-app-rewired": "^1.5.2",
    "sass-loader": "^7.0.1"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "codemirror": "^5.38.0",
    "copy-to-clipboard": "^3.0.8",
    "custom-react-scripts": "^0.2.2",
    "deepdetect-js": "^1.5.2",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "node-sass-chokidar": "^1.3.0",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-typeahead": "^3.1.3",
    "react-bootstrap4-modal": "^1.3.1-pre1",
    "react-bounding-box": "^0.2.6",
    "react-codemirror2": "^5.0.2",
    "react-dom": "^16.3.2",
    "react-json-table": "^0.1.1",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^7.0.4",
    "react-tooltip": "^3.6.0",
    "store": "^2.0.12",
    "superagent": "^3.8.3",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "precommit": "pretty-quick --staged",
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev:install": "./script/00-install-dev.sh",
    "dev:up": "docker-compose -f docker/docker-compose.dev.yml up",
    "dev:down": "docker-compose -f docker/docker-compose.dev.yml down",
    "dev:uninstall": "docker-compose -f docker/docker-compose.dev.yml down --rmi all",
    "dev:connect:deepdetect": "docker exec -it target_api_1 /bin/sh",
    "eris:install": "./script/00-install-eris.sh",
    "eris:up": "docker-compose -f docker/docker-compose.eris.yml up",
    "eris:down": "docker-compose -f docker/docker-compose.eris.yml down",
    "eris:uninstall": "docker-compose -f docker/docker-compose.eris.yml down --rmi all"
  }
}
