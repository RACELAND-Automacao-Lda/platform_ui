{
  "name": "core-ui",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "node-sass": "^4.9.0",
    "prettier": "^1.13.0",
    "pretty-quick": "^1.6.0",
    "react-app-rewired": "^1.5.2",
    "sass-loader": "^7.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.0.20",
    "bootstrap": "^4.1.1",
    "codemirror": "^5.38.0",
    "copy-to-clipboard": "^3.0.8",
    "custom-react-scripts": "^0.2.2",
    "deepdetect-js": "^1.2.4",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-typeahead": "^3.1.3",
    "react-bounding-box": "^0.2.2",
    "react-codemirror2": "^5.0.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-tooltip": "^3.6.0",
    "superagent": "^3.8.3",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "precommit": "pretty-quick --staged",
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "docker-build": "docker build -t core-ui .",
    "docker-run": "docker run -it -v ${PWD}:/usr/src/app -v /usr/src/app/node_modules -p 3000:3000 --rm core-ui"
  }
}
